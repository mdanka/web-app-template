  "scripts": {
    "build": "npm-run-all createBuildDir -s bundle:prod",
    "buildDev": "npm-run-all createBuildDir -s bundle:dev ",
    "clean": "rimraf dist",
    "start": "npm-run-all createBuildDir -p watch:server",
    "watch": "npm-run-all createBuildDir -p watch:bundle",
    "bundle:dev": "webpack -d",
    "bundle:prod": "webpack --config webpack.config.js",
    "createBuildDir": "mkdirp dist",
    "watch:bundle": "webpack -d -w",
    "watch:server": "webpack-dev-server -d --config webpack.config.server.js"
  },
  "dependencies": {
    "@blueprintjs/core": "^1.4.0",
    "commonjs": "^0.0.1",
    "es6-shim": "^0.35.2",
    "lodash": "^4.17.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-logger": "^1.1.0",
    "react-redux": "^5.0.1",
    "react-router-redux": "^4.0.7",
    "react-select": "^1.0.0-rc.2",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.0",
    "autoprefixer-loader": "^3.2.0",
    "css-loader": "^0.26.1",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "handlebars": "^4.0.6",
    "html-webpack-plugin": "^2.24.1",
    "imports-loader": "^0.7.0",
